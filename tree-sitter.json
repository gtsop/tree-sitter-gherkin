{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gherkin",
      "camelcase": "Gherkin",
      "title": "Gherkin",
      "scope": "source.gherkin",
      "file-types": [
        "gherkin"
      ],
      "injection-regex": "^gherkin$",
      "class-name": "TreeSitterGherkin"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "AGPLv3",
    "description": "Gherkin grammar for tree-sitter",
    "authors": [
      {
        "name": "George Tsopanoglou",
        "email": "gtsop+github@protonmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/gtsop/tree-sitter-gherkin.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}