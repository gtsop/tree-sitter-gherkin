{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gherkin",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature_definition"
        }
      ]
    },
    "feature_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_feature_line"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_background_line"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_scenario_line"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "background": {
      "type": "PATTERN",
      "value": "Background"
    },
    "feature": {
      "type": "PATTERN",
      "value": "Feature"
    },
    "scenario": {
      "type": "PATTERN",
      "value": "Scenario"
    },
    "text": {
      "type": "PATTERN",
      "value": "[a-zA-Z ]+"
    },
    "_background_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "background"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_new_line"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_feature_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature"
        },
        {
          "type": "STRING",
          "value": ": "
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_new_line"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_scenario_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "scenario"
        },
        {
          "type": "STRING",
          "value": ": "
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_new_line"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_new_line": {
      "type": "STRING",
      "value": "\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}