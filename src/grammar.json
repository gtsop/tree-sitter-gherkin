{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gherkin",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature_definition"
        }
      ]
    },
    "feature_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "feature": {
      "type": "PATTERN",
      "value": "Feature"
    },
    "text": {
      "type": "PATTERN",
      "value": "[a-zA-Z ]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}