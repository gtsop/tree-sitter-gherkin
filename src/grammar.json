{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gherkin",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature_declaration"
        }
      ]
    },
    "feature_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Feature"
        },
        {
          "type": "STRING",
          "value": ": "
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "title": {
      "type": "PATTERN",
      "value": ".*\\n"
    },
    "description": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[\\s\\S]+"
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}