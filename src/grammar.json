{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gherkin",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "feature"
        }
      ]
    },
    "feature": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Feature: "
        },
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "scenario"
        }
      ]
    },
    "title": {
      "type": "PATTERN",
      "value": ".+\\n"
    },
    "description": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "description_line"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "description_line": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": ".+"
        }
      }
    },
    "scenario": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Scenario: "
        },
        {
          "type": "SYMBOL",
          "name": "title"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}